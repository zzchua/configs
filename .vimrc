set background=dark
set pastetoggle=<F2>
set nocompatible
set backspace=indent,eol,start
set lbr nu ts=4 sw=4 ai si sc bs=2 wb nobk vb so=1 ru ls=2 ww=b,s,h,l,<,>,[,]
set com=sr:/*,mb:*,el:*/,://,b:#,:XCOMM,n:>,fb:-,n:\:
set nojs vi=""
set mps+=<:>
set clipboard=autoselect,exclude:.*
set nowrap
set softtabstop=0
set undolevels=1000
set undoreload=10000
syntax enable
